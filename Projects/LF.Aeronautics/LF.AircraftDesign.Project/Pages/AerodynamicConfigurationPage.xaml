<UserControl x:Class="LF.AircraftDesign.Project.Pages.AerodynamicConfigurationPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LF.AircraftDesign.Project.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="4">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="2*" />
            <RowDefinition Height="2*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <GroupBox Header="主视图" Margin="4" Grid.Row="1" Grid.RowSpan="2"></GroupBox>
        <GroupBox Header="左视图" Margin="4" Grid.Row="1" Grid.Column="1"></GroupBox>
        <GroupBox Header="侧视图" Margin="4" Grid.Row="2" Grid.Column="1" >
        </GroupBox>

    </Grid>
</UserControl>
